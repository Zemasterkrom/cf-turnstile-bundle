{
    "name": "zemasterkrom/zmkr-cloudflare-turnstile-bundle",
    "description": "Extensible and configurable integration of Cloudflare Turnstile with Symfony",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "cloudflare",
        "turnstile",
        "captcha",
        "form",
        "security",
        "spam",
        "anti-spam"
    ],
    "authors": [
        {
            "name": "Zemasterkrom",
            "email": "zemasterkrom@outlook.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zemasterkrom\\CloudflareTurnstileBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zemasterkrom\\CloudflareTurnstileBundle\\Test\\": "tests/"
        }
    },
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4|^6.0"
    }
}

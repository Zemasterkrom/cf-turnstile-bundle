{
    "name": "zemasterkrom/zmkr-cloudflare-turnstile-bundle",
    "description": "Extensible and configurable integration of Cloudflare Turnstile with Symfony",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "cloudflare",
        "turnstile",
        "captcha",
        "form",
        "security",
        "spam",
        "anti-spam"
    ],
    "authors": [
        {
            "name": "Zemasterkrom",
            "email": "zemasterkrom@outlook.fr"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/asset": "^5.0|^6.0|^7.0",
        "symfony/form": "^5.0|^6.0|^7.0",
        "symfony/framework-bundle": "^5.0|^6.0|^7.0",
        "symfony/http-client": "^5.0|^6.0|^7.0",
        "symfony/translation": "^5.0|^6.0|^7.0",
        "symfony/twig-bundle": "^5.0|^6.0|^7.0",
        "symfony/validator": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "dg/bypass-finals": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "symfony/panther": "^0.8|^1.0|^2.0",
        "symfony/phpunit-bridge": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zemasterkrom\\CloudflareTurnstileBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zemasterkrom\\CloudflareTurnstileBundle\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpunit": "./vendor/bin/simple-phpunit",
        "phpstan": "./vendor/bin/phpstan analyse"
    },
    "minimum-stability": "dev"
}